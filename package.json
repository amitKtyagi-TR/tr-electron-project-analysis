{
  "name": "@ai-assistant/code-analysis-engine",
  "version": "0.1.0",
  "description": "Node.js/TypeScript code analysis engine for AI coding assistant",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "example:git": "tsx examples/test-git-processor.ts",
    "example:files": "tsx examples/file-processing-demo.ts",
    "setup:parsers": "node scripts/download-parsers.js"
  },
  "keywords": [
    "code-analysis",
    "typescript",
    "ast",
    "electron",
    "static-analysis"
  ],
  "author": "AI Assistant Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.1",
    "@types/p-limit": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "p-limit": "^5.0.0",
    "web-tree-sitter": "^0.20.8"
  }
}
